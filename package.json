{
  "name": "pino-pretty-signale",
  "version": "0.1.0",
  "description": "Pretty print pino log lines with style adopted from signale",
  "repository": "https://github.com/RanolP/pino-pretty-signale.git",
  "author": "RanolP <public.ranolp@gmail.com>",
  "license": "MIT",
  "type": "module",
  "bin": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    "./runtime": {
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.cjs"
    },
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "start": "pino-pretty-signale"
  },
  "devDependencies": {
    "@types/dateformat": "^5.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.7.18",
    "@types/pump": "^1.1.1",
    "colorette": "^2.0.19",
    "dateformat": "^4.6.3",
    "fast-copy": "^2.1.3",
    "fast-safe-stringify": "^2.1.1",
    "help-me": "^4.1.0",
    "joycon": "^3.1.1",
    "minimist": "^1.2.6",
    "nanobundle": "^0.0.28",
    "pino-abstract-transport": "^1.0.0",
    "pump": "^3.0.0",
    "readable-stream": "^4.1.0",
    "secure-json-parse": "^2.5.0",
    "sonic-boom": "^3.2.0",
    "strip-json-comments": "^5.0.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "on-exit-leak-free": "^2.1.0"
  }
}
