{
  "name": "pino-pretty-signale",
  "version": "0.1.0",
  "description": "Pretty print pino log lines with style adopted from signale",
  "repository": "https://github.com/RanolP/pino-pretty-signale.git",
  "author": "RanolP <public.ranolp@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "bin": "dist/esm/cli/index.js",
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./runtime": {
      "import": "./dist/esm/runtime.js",
      "require": "./dist/cjs/runtime/index.cjs"
    },
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "tsm build.ts",
    "build:types": "tsc -p tsconfig.types.json",
    "watch:esm": "pnpm run build:esm --watch",
    "start": "tsm src/cli/index.ts",
    "dev": "nodemon --exec \"pnpm start\""
  },
  "devDependencies": {
    "@types/dateformat": "^5.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.8.4",
    "@types/pump": "^1.1.1",
    "chokidar": "^3.5.3",
    "colorette": "^2.0.19",
    "concurrently": "^7.4.0",
    "dateformat": "^4.6.3",
    "deepmerge": "^4.2.2",
    "esbuild": "^0.15.10",
    "esbuild-plugin-copy": "^1.3.0",
    "fast-copy": "^2.1.3",
    "fast-glob": "^3.2.12",
    "fast-safe-stringify": "^2.1.1",
    "help-me": "^4.1.0",
    "joycon": "^3.1.1",
    "minimist": "^1.2.7",
    "nodemon": "^2.0.20",
    "on-exit-leak-free": "^2.1.0",
    "pino-abstract-transport": "^1.0.0",
    "pump": "^3.0.0",
    "readable-stream": "^4.2.0",
    "secure-json-parse": "^2.5.0",
    "sonic-boom": "^3.2.0",
    "strip-json-comments": "^3.1.1",
    "tsm": "^2.2.2",
    "typescript": "^4.8.4"
  }
}
